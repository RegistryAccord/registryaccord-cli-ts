{
    "name": "registryaccord-cli-ts",
    "version": "0.1.0",
    "type": "module",
    "bin": {
        "ra": "bin/run.js"
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "dev": "node --enable-source-maps bin/dev.js",
        "lint": "eslint src/ --ext .ts",
        "test": "vitest run",
        "test:watch": "vitest",
        "prepack": "npm run build"
    },
    "dependencies": {
        "@noble/ed25519": "^3.0.0",
        "@oclif/core": "^4.0.0",
        "@types/fs-extra": "^11.0.4",
        "bs58": "^6.0.0",
        "fs-extra": "^11.3.2"
    },
    "devDependencies": {
        "@types/node": "^20.11.30",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.0.0",
        "eslint": "^8.56.0",
        "typescript": "^5.4.0",
        "vitest": "^1.6.0"
    },
    "oclif": {
        "commands": {
            "strategy": "pattern",
            "target": "./dist/commands"
        },
        "bin": "ra"
    },
    "engines": {
        "node": ">=20"
    }
}
